                "minCap"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "minCap"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_type"
            ],
            "APPLY": [
                {
                    "minCap": {
                        "MIN": "rooms_seats"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
    "result":[
