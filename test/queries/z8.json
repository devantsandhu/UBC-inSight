{
    "title": "MIN, MAX, COUNT and AVG number of seats for each type of room in BUCH",
    "query": {
        "WHERE": {
            "IS": {
                "rooms_name": "BUCH_*"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_type",
                "minCapacity",
                "maxCapacity",
                "avgCapacity",
                "count"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "minCapacity",
                    "maxCapacity",
                    "avgCapacity",
                    "count"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_type"
            ],
            "APPLY": [
                {
                    "minCapacity": {
                        "MIN": "rooms_seats"
                    }
                },
                {
                    "maxCapacity": {
                        "MAX": "rooms_seats"
                    }
                },
                {
                    "avgCapacity": {
                        "AVG": "rooms_seats"
                    }
                },
                {
                    "count": {
                        "COUNT": "rooms_seats"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
